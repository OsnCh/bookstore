{
  "name": "nest-typescript-starter",
  "version": "1.0.0",
  "description": "Nest TypeScript starter repository",
  "license": "MIT",
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "build:netlify": "npm run build && node ./tsconfig-paths-bootstrap.js",
    "before-start": "rimraf dist && npm run build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "node dist/main.js",
    "start:dev": "concurrently \"wait-on dist/main.js && nodemon\" \"tsc -w -p tsconfig.json\" ",
    "start:docker": "nodemon --exec \"ts-node\" --cache-directory .tscache src/main.ts --port=3001",
    "start:staging": "npm run before-start && npm run server:staging",
    "server:staging": "node -r ./tsconfig-paths-bootstrap.js dist/main.js",
    "start:prod": "npm run server:prod",
    "server:prod": "node -r ./tsconfig-paths-bootstrap.js dist/main.js",
    "heroku-postbuild": "npm install --only=dev --no-shrinkwrap && npm run build"
  },
  "dependencies": {
    "@nestjs/common": "^6.1.1",
    "@nestjs/core": "^6.1.1",
    "@nestjs/jwt": "6.0.0",
    "@nestjs/microservices": "^6.1.1",
    "@nestjs/passport": "6.0.0",
    "@nestjs/platform-express": "^6.1.1",
    "@nestjs/swagger": "3.0.2",
    "@nestjs/typeorm": "6.0.0",
    "@nestjs/websockets": "^6.1.1",
    "class-transformer": "0.2.0",
    "class-validator": "0.9.1",
    "crypto-js": "^3.1.9-1",
    "fb": "^2.0.0",
    "google-auth-library": "^4.2.2",
    "lodash": "^4.17.11",
    "lodash-cli": "^4.17.5",
    "logzio-nodejs": "^1.0.5",
    "mongodb": "3.2.3",
    "nodemailer": "^6.2.1",
    "passport": "0.4.0",
    "passport-facebook": "^3.0.0",
    "passport-http-bearer": "1.0.1",
    "passport-jwt": "4.0.0",
    "reflect-metadata": "0.1.13",
    "rxjs": "6.5.1",
    "serverless-http": "^2.3.0",
    "smtp-client": "^0.3.1",
    "ts-md5": "^1.2.4",
    "typeorm": "0.2.16",
    "typescript": "3.4.5"
  },
  "devDependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/preset-env": "^7.4.5",
    "@types/jest": "24.0.11",
    "@types/node": "10.14.4",
    "babel-plugin-lodash": "^3.3.4",
    "jest": "24.5.0",
    "nodemon": "^1.18.10",
    "supertest": "4.0.2",
    "swagger-ui-express": "^4.0.2",
    "ts-jest": "24.0.0",
    "ts-nameof": "^3.0.3",
    "ts-node": "8.0.3",
    "tsconfig-paths": "^3.8.0",
    "tslint": "5.14.0"
  }
}
